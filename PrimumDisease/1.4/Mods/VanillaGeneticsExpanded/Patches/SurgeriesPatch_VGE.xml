<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
<!--Basic-->
<!--RemoveBodyPart-->
<!--CureAilment-->
<!--CureAilmentComplex-->
<!--CureAilmentSpecial-->
<!--ImplantBasic-->
<!--ImplantProsthetic-->
<!--ImplantProstheticComplex-->
<!--ImplantNatural-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[@Name = "GR_InstallBearClaws" or @Name = "GR_InstallDiggingBearClaws" or @Name = "GR_InstallBearMuscleTissue" or @Name = "GR_InstallOvipositor" or @Name = "GR_InstallMuffaloMuscles" or @Name = "GR_InstallMuffaloMammaries" or @Name = "GR_InstallMuffaloSkin" or @Name = "GR_InstallBoomalopeNodules" or @Name = "GR_InstallChemfuelStomach" or @Name = "GR_InstallWolfMuscularFibers" or @Name = "GR_InstallDevouringJaws" or @Name = "GR_InstallMousePheromones" or @Name = "GR_InstallMoleratNerveDampener" or @Name = "GR_InstallBunnyEar" or @Name = "GR_InstallInsectoidAntennae" or @Name = "GR_InstallPsychicDampeners" or @Name = "GR_InstallKidneyToxicFilters" or @Name = "GR_InstallVenomousFangs" or @Name = "GR_InstallIguanaScales"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>2500</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.05</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>0.9</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>10</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>2</count>
				</value>
			</li>
		</operations>
	</Operation>
<!--ImplantNaturalComplex-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[@Name = "GR_InstallChemfuelHeart" or @Name = "GR_InstallCompoundEye" or @Name = "GR_InstallCatEye"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>3500</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.1</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>0.9</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>13</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>4</count>
				</value>
			</li>
		</operations>
	</Operation>
<!--ImplantNaturalSpecial-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[@Name = "GR_InstallHibernationModule" or @Name = "GR_InstallNeuronReinforcement" or @Name = "GR_InstallHumanThrumboHorn" or @Name = "GR_InstallThrumboSkin"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>4000</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.15</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>0.9</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>15</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>6</count>
				</value>
			</li>
		</operations>
	</Operation>
<!--ImplantBionic-->
<!--ImplantBionicComplex-->
<!--ImplantBionicSpecial-->
			</operations>
		</match>
	</Operation>
</Patch>
