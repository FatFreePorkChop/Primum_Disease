<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
<!--Basic-->
<!--RemoveBodyPart-->
<!--CureAilment-->
<!--CureAilmentComplex-->
<!--CureAilmentSpecial-->
<!--ImplantBasic-->
<!--ImplantProsthetic-->
<!--ImplantProstheticComplex-->
<!--ImplantNatural-->
<!--ImplantNaturalComplex-->
<!--ImplantNaturalSpecial-->
<!--ImplantBionic-->
<!--ImplantBionicComplex-->
<!--ImplantBionicSpecial-->
<!--FertilityBasic-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[defName = "Vasectomy" or defName = "ImplantIUD" or defName = "RemoveIUD" or defName = "ImplantEmbryo" or defName = "TerminatePregnancy"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>2000</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>1.00</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>5</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>2</count>
				</value>
			</li>
		</operations>
	</Operation>

<!--FertilityComplex-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[defName = "Tubal Ligation" or defName = "ReverseVasectomy" or defName = "ExtractOvum"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>3000</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.1</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>1.00</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>10</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>4</count>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/fixedIngredientFilter</xpath>
				<key>disallowedThingDefs</key>
				<value>
					<li>MedicineHerbal</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>
