<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!--Basic-->
<!--RemoveBodyPart-->
	<Operation Class="PrimumDisease.PatchOperationContext">
		<xpath>/Defs/RecipeDef[defName = "RemoveBodyPart"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>2000</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.05</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>2</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>4</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>1</count>
				</value>
			</li>
		</operations>
	</Operation>

