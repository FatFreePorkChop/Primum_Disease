<!--Basic-->
<!--RemoveBodyPart-->
<!--CureAilment-->
<!--CureAilmentComplex-->
<!--CureAilmentSpecial-->
	<li Class="PatchOperation.Context">
		<xpath>/Defs/RecipeDef[defName = "CureAbasia" or defName = "CureBloodRot"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>2000</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.25</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>1.5</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>10</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>10</count>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/fixedIngredientFilter</xpath>
				<key>disallowedThingDefs</key>
				<value>
					<li>MedicineHerbal</li>
					<li>MedicineIndustrial</li>
				</value>
			</li>
		</operations>
	</li>
