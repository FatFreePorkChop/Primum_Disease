<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Save Our Ship</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
<!--Basic-->
<!--RemoveBodyPart-->
<!--CureAilment-->
<!--CureAilmentComplex-->
<!--CureAilmentSpecial-->
<!--ImplantBasic-->
<!--ImplantProsthetic-->
<!--ImplantProstheticComplex-->
<!--ImplantNatural-->
	<Operation Class="PatchOperation.Context">
		<xpath>/Defs/RecipeDef[defName = "GR_InstallBearClaws" or defName = "GR_InstallDiggingBearClaws" or defName = "GR_InstallBearMuscleTissue" or defName = "GR_InstallOvipositor" or defName = "GR_InstallMuffaloMuscles" or defName = "GR_InstallMuffaloMammaries" or defName = "GR_InstallMuffaloSkin" or defName = "GR_InstallBoomalopeNodules" or defName = "GR_InstallChemfuelStomach" or defName = "GR_InstallWolfMuscularFibers" or defName = "GR_InstallDevouringJaws" or defName = "GR_InstallMousePheromones" or defName = "GR_InstallMoleratNerveDampener" or defName = "GR_InstallBunnyEar" or defName = "GR_InstallInsectoidAntennae" or defName = "GR_InstallPsychicDampeners" or defName = "GR_InstallKidneyToxicFilters" or defName = "GR_InstallVenomousFangs" or defName = "GR_InstallIguanaScales"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>2500</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.05</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>0.9</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>10</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>2</count>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/fixedIngredientFilter</xpath>
				<key>disallowedThingDefs</key>
				<value>
				</value>
			</li>
		</operations>
	</Operation>
<!--ImplantNaturalComplex-->
	<Operation Class="PatchOperation.Context">
		<xpath>/Defs/RecipeDef[defName = "GR_InstallChemfuelHeart" or defName = "GR_InstallCompoundEye" or defName = "GR_InstallCatEye"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
				<value>3500</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>deathOnFailedSurgeryChance</key>
				<value>0.1</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>surgerySuccessChanceFactor</key>
				<value>0.9</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>skillRequirements</key>
				<value>
					<Medicine>13</Medicine>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/ingredients</xpath>
				<key>li[1]</key>
				<value>
					<filter>
						<categories>
							<li>Medicine</li>
						</categories>
					</filter>
					<count>4</count>
				</value>
			</li>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef/fixedIngredientFilter</xpath>
				<key>disallowedThingDefs</key>
				<value>
				</value>
			</li>
		</operations>
	</Operation>
<!--ImplantNaturalSpecial-->
	<Operation Class="PatchOperation.Context">
		<xpath>/Defs/RecipeDef[defName = "GR_InstallHibernationModule" or defName = "GR_InstallNeuronReinforcement" or defName = "GR_InstallHumanThrumboHorn" or defName = "GR_InstallThrumboSkin"]</xpath>
		<operations>
			<li Class="PrimumDisease.PatchOperationAddOrReplace">
				<xpath>/RecipeDef</xpath>
				<key>workAmount</key>
